	Keywords = {Acoustic wave,Blow up,Energy decay,Exponential growth,Finite time,Rate},
	Number = {9},
	Pages = {4898--4941},
	Publisher = {Elsevier Inc.},
	Title = {{On the wave equation with semilinear porous acoustic boundary conditions}},
	Url = {http://dx.doi.org/10.1016/j.jde.2012.01.042},
